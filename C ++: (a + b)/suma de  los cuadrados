/*3. Realice un programa que calcule y muestre en la salida est√°ndar la suma de 
los cuadrados de los 10 primeros enteros mayores que cero.*/

#include <Arduino.h>

int suma=0, cuadrado;
void setup() {
  Serial.begin(9600);
   for (int i = 1; i <= 10; i++)
 {
   	cuadrado = i * i;
		suma += cuadrado;
 }
 
  Serial.println("ln La suma de los cuadrados es" + String(suma));

}


void loop() {
  

  
}
